rule "r1a - Budget question between 300 and 600"
    no-loop
    when
        eval(UI.answerBudget(Questions.CHOOSE_BUDGET,300,600))
    then
        Hypothesis $hypothesis = new Hypothesis();
        $hypothesis.budgetType = Hypothesis.BudgetType.BAIXO;
        insert($hypothesis);
end


