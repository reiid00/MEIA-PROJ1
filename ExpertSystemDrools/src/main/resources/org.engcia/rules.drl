        $kb.setMinStorage(240);
        $kb.setMinRAMPreferred(8);
        $kb.setMinRAMSpeed(2600);
        $kb.setAdequateMinCPUBenchmark(120);
        $kb.setDurationDayChoice(1);

        // 	insert(h);
end

