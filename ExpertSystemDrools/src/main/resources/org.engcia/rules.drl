        Hypothesis( description == "Finalidade", value == "NAVEGACAO_WEB_SIMPLES")
        eval(UI.answer(Questions.CHOOSE_FILE_TYPE, "1"))
        eval(UI.answer(Questions.CHOOSE_DURATION_PER_DAY, "1"))
	then
// 	  KnowledgeBase h = new KnowledgeBase();
